#include "main.h"

 FFT_HANDLER  searcher;

int main(){

	uint8_t  max_index;
	searcher.Init();
	while(1){
		max_index=searcher.Read_Index();
		if (max_index!=0){
			printf("max_index %d : ",max_index);
			usleep(1000);
		}
	}
	 return 0;
}





